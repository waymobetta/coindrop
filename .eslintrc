{
  "settings": {
    "react": {
      "version": "16.6.3"
    }
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "rules": {
    "strict": 0,
		"react/prop-types": 0,
    "no-console": "off"
  },
  "extends": ["eslint:recommended", "plugin:react/recommended"]
}
